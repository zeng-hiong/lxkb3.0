/*common*/
div,p,span,a,button,input,img{font-size: 0.24rem;-webkit-tap-highlight-color: rgba(0,0,0,0)}
.mui-media-body{position: relative;}
.mui-input-row .mui-icon-arrowright{
	right: 15px;
	position: absolute;
	top: 50%;
	transform: translateY(-50%);
    -webkit-transform: translateY(-50%);
}
.mui-table-view-cell > a:not(.mui-btn) p{font-size: 0.3rem;}
*{-webkit-touch-callout:none;
      touch-callout:none;}
.purecl{background: #00adff;}
.mui-bar.purecl{background: #00adff;}
.letterbtn{width:0.48rem;position: absolute;right:0.36rem;top:0.5rem;z-index: 10;}
.letterbtn img{width:100%}
.letterbtn span{position:absolute;top:0px;border-radius: 50%;background: red;right:-2px;width:0.16rem;height:0.16rem;}
.mui-pull .mui-pull-caption{color:#309bdf}
.flexbox{
	 display: -webkit-box;display: -webkit-flex;display: flex;
}
.spacebt{
	justify-content: space-between;
}
.mui-input-row,.mui-input-row a,.mui-input-row input,.mui-input-row span,.mui-input-row div{
	font-size: 0.3rem;
}
.itemcenter{
	align-items: center;
}
.spacear{
	justify-content: space-around;
}

.abvt{
	position: absolute;
	top:50%;
	transform:translateY(-50%);
	-webkit-transform: translateY(-50%);
}
.aligncenter{
        text-align: center;
    }
.alignleft{
        text-align: left;
    }
.alignright{
        text-align: right;
    }
.bankimg{  width: 0.54rem;
        margin-right: 0.1rem}
.newbtn{
	font-size: 0.32rem;
    text-align: center;
    background-image: linear-gradient(90deg, #008aff 0%, #00b4ff 100%);
   background-image: -webkit-linear-gradient(90deg, #008aff 0%, #00b4ff 100%);
    color: #fff;
    margin: 0 auto;
    padding: 0.24rem 0;
    width: 78%;
    border-radius:0.2rem;
    box-shadow: 0px 4px 14px 0px 
		rgba(0, 159, 255, 0.44);
    margin-top: 0.6rem;
    display: block;
    -webkit-user-select:none;
    user-select:none;
}
.verfity{
	position: absolute;
	z-index: 999;
	top:50%;
	right:15px;
	width:1.7rem;
	text-align: center;
	-webkit-transform: translateY(-50%);
	transform: translateY(-50%);
     background-image: -webkit-linear-gradient(0deg, #008aff 0%, #00b4ff 100%);
	background-image: linear-gradient(0deg, #008aff 0%, #00b4ff 100%);
	border-radius: 5px;
	padding:4px 6px;
}
.verfity a{color: #fff;font-size: 0.24rem;}
