/*accountCenter*/
.accounttop{
	display: -webkit-box;
    display: -webkit-flex;
    display: flex;
    -webkit-box-align: center;
    -webkit-align-items: center;
    align-items: center;
    background-image: url(../image/pic_bg_my_2.png);
    background-repeat: no-repeat;
    background-size: 100% 100%;
    font-size: 0.3rem;
    padding: 0.8rem 0.35rem 0.5rem;
    color: #fff;
}
.agentbg.accounttop{
	background-image: url(../image/pic_bg_my.png);
    background-repeat: no-repeat;
    background-size: 100% 104%;
}
.touxiang{
	width: 1.56rem;
    height: 1.56rem;
    border-radius: 50%;
    margin-right: 0.2rem;
    overflow: hidden;
}
.touxiang img{width: 100%;height: 100%;}
.accounttop .userinfo{
	font-size: 0.28rem;
	flex:2;
}
.accounttop .userinfo p{margin-bottom: 0.1rem;}
.accounttop .userinfo span{
	margin-left: 0.1rem;
	padding:1px 0.1rem;
	color:#fff;
	border-radius: 5px;
	font-size: 0.2rem;
	position: relative;
}
.accounttop .userinfo span.notyet{
	background: #8A6DE9;
}
.accounttop .userinfo span.notard{
	background:#20d453;
}
.accounttop .userinfo span.agent{
	border:1px solid #fdd227;
	padding-left: 0.4rem;
	box-shadow: 0 1px 0.1rem #10911b;
}
.accounttop .userinfo span.agent img{width: 0.28rem;left: 0.1rem;}
.phonenum,.visition,.rightspan{
	position: absolute;
	right: 0;
	top:50%;
	-webkit-transform: translateY(-50%);
	transform: translateY(-50%);
	color:#999
}
.rightspan span{color:red}
/*密码框*/
#paypassword{
    width: 100%;
    height: 100%;
    background: rgba(0,0,0,0.4);
    z-index: 8;
    position: absolute;
    top: 0;
}
#paypassword>div{
  width: 100%;
  position: absolute;
  bottom:0;
  background: #fff;
}
#paypassword>div>div:first-child{
	padding:0.2rem 0.3rem;
	border-bottom: 1px solid #C8C7CC;
}
#paypassword>div>div:first-child p{color:#999}
#paypassword>div>div:first-child p:first-child{
	color:#333;
	font-size: 0.3rem;
	margin-bottom: 0.2rem;
}
#paypassword>div>p{
    padding:0.2rem 0.35rem;
    font-size: 0.3rem;
    position: relative;
    text-align: center;
    border-bottom: 1px solid #e4e4e4;
}
#paypassword>div>p .arrow{
    transform:rotate(180deg) translateY(40%);
    top:50%;
    position: absolute;
    left: 0.35rem;
    margin-bottom: -0.06rem
}
#paypassword .pwd-box{
    width:5.4rem;
    position: relative;
    border: 1px solid #999;
    border-radius: 0.06rem;
    margin:0.3rem auto 0;
 }
#paypassword .fake-box{
    font-size: 0;
}
#paypassword .pwd-box .pwd-input{
    width: 99%;
    height:0.9rem;
    color: transparent;
    position: absolute;
    top: 0;
    left: 0;
    border: none;
    font-size: 0.3rem;
    opacity: 0;
    z-index: 1;
}
#paypassword .fake-box input{
    width: 16.6%;
    height:0.9rem;
    line-height: 0.9rem;
    border-radius: 0;
    border: none;
    border-right: 1px solid #999;
    text-align: center;
    font-size: 30px;
    margin:0;
    padding:0;
}
#paypassword .fake-box input:nth-last-child(1){
     border:none;
 }
 #paypassword table img{
    width:0.76rem; 
    margin-bottom: 0.08rem
 }
 #paypassword table{
    border-top:1px solid #e4e4e4;
    font-size: 0.5rem;
    text-align: center;
 }
 #paypassword table td{
    width:2.5rem;
    height: 1.3rem;
    border-right:1px solid #e4e4e4;
    border-bottom:1px solid #e4e4e4
 }
  #paypassword table .delt,#paypassword .empty{
    background:#dcdcdc; 
  }
 /* 数字计算器*/
#calculator{
   font-size: 0.5rem;
   background: #fff;
   text-align: center;
   z-index: 2;
   position: absolute;
   bottom:0;
}
#calculator td{
    width:1.875rem;
    height: 1.3rem;
    border-right:1px solid #e4e4e4;
    border-bottom:1px solid #e4e4e4;
}
#calculator .makesure{
    background: #309bdf;
    font-size: 0.44rem;
    color: #fff;
}
#calculator .del{
    background: #3b2e33;
    border-bottom: none;
}
#calculator .del img{
    width:0.76rem; 
    margin-bottom: 0.08rem
}
/*还款详情*/
.formbox {
    margin-bottom: 0.3rem;
    padding: 0 0.25rem;
    background: #fff;
    font-size: 0.3rem;
}
.formbox>div {
    display: -webkit-box;
    display: -webkit-flex;
    display: -moz-box;
    display: -ms-flexbox;
    display: flex;
    justify-content: space-between;
    border-bottom: 1px solid #e4e4e4;
    -webkit-box-align: center;
    -webkit-align-items: center;
    -moz-box-align: center;
    -ms-flex-align: center;
    align-items: center;
}
.repaydetail div {
    padding: 0.1rem 0.2rem;
}
.formbox div:last-child {
    border-bottom: none;
}
.repaydetail .detiltop {
    color: #666;
    padding: 0;
}
.formbox.repaydetail {
    padding: 0;
}
.repaydetail img {
    margin-top: -0.04rem;
    width:0.4rem
}
.repaydetail .dateti {
    color: #309bdf;
}
.repaydetail .detiltop>span {
    padding: 0 0.2rem;
    position: relative;
}

.formbox.repaydetail div .spanbox {
    padding: 0.1rem 0.4rem;
}
.repaydetail  span{font-size: 0.3rem;vertical-align: middle;}
.repaydetail  span span{vertical-align: baseline;}
.spendType {
	 background-image: -webkit-linear-gradient(0deg,#008aff 0%,#00b4ff 100%);
     background-image: linear-gradient(0deg,#008aff 0%,#00b4ff 100%);
    color: #fff;
    border-radius: 0.2rem;
    padding: 0.02rem 0.14rem;
}
.repaydetail .detiltop .first::after {
    width: 1px;
    height: 0.25rem;
    position: absolute;
    left: 100%;
    background: #e4e4e4;
    content: '';
    display: block;
    top: 50%;
    -webkit-transform: translateY(-50%);
    transform: translateY(-50%);
}
.spanbox .time {
    color: #666;
    margin: 0 0.4rem 0 0.1rem;
}
.repayType {
   
    background-image: -wenkit-linear-gradient(0deg,#fa3966 0%, #fb5078 54%,#fc678a 100%);
    background-image: linear-gradient(0deg,#fa3966 0%, #fb5078 54%,#fc678a 100%);
    color: #fff;
    border-radius: 0.2rem;
    padding: 0.02rem 0.14rem;
}
.choosecard {
    position: absolute;
    bottom: 0;
    width: 100%;
    z-index: 4;
}
.choosecard>p {
    padding: 0.2rem;
    border-bottom: 1px solid #edf4f9;
    background: #fff;
    font-size: 0.3rem;
    position: relative;
}
.choosecard .formbox {
    margin-bottom: 0;
}
.choosecard .formbox >div{padding:0.24rem 0.1rem}
.choosecard>p .abvt {
    left: 0.35rem;
    color: #309bdf;
    font-size: 0.3rem;
}
.choosecard input[type='radio']:checked{
				background: url(../image/icon_sel.png);
				background-size:100% 100% ;
			}
.choosecard img {
    margin-right: 0.2rem;
}
.choosecard .formbox label {
    flex: 2;
}
.choosecard .formbox input {
    width: 0.4rem;
    height: 0.4rem;
     background: url(../image/icon_disel.png);
     background-size:100% 100%;
}